
public class Question4 {
	public static void main(String[] args) {
		int n=11;
		int result = (n-1)/2;
		
		for(int i=0;i<n;i++) {
			for(int j =0;j<n;j++) {
				if((j==0 && i>=(n-1)/2) ||(j==n-1 && i>=(n-1)/2) || i==n-1 || i==n-2 ) {
					System.out.print("*");
				}
				else {
					System.out.print(" ");
				}
				
			}
			System.out.println("");
	       // System.out.println(result);
		}
	}
	}


